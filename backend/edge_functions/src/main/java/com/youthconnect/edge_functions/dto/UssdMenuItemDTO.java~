package com.youthconnect.edge_functions.dto;

import lombok.AllArgsConstructor;

/**
 * USSD Menu Item DTO
 * Individual menu option
 */
@Data
@Builder
@NoArgsConstructor
@AllArgsConstructor
public class UssdMenuItemDTO {
    private String key;
    private String label;
    private String action;
    private String nextMenu;
}