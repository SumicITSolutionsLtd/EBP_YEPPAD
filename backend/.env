# Africa's Talking (SMS & USSD)
AFRICASTALKING_USERNAME=sandbox
AFRICASTALKING_API_KEY=<CHANGE_IN_PRODUCTION>
USSD_SERVICE_CODE=*256#

# SMTP Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=<CHANGE_IN_PRODUCTION>
SMTP_PASSWORD=<CHANGE_IN_PRODUCTION>
SMTP_FROM=noreply@youthconnect.ug

# AWS S3 (if STORAGE_TYPE=S3)
AWS_ACCESS_KEY_ID=<CHANGE_IN_PRODUCTION>
AWS_SECRET_ACCESS_KEY=<CHANGE_IN_PRODUCTION>
AWS_REGION=eu-west-1
AWS_S3_BUCKET=youthconnect-uploads

